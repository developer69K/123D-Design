						<markMenu name="CustomizeMaterialBrowser">
							<markMenuItem text="Apply On Click" value="JanusMaterial.ApplyOnClick" position="overflow"/>
							<markMenuItem text="Select On Click" value="JanusMaterial.SelectOnClick" position="overflow"/>

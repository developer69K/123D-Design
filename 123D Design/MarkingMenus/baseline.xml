							<markMenuSeparator/>
							<markMenuItem text="Enable In-Scene Material Filter" value="JanusMaterial.EnableInSceneMaterialsFilter" position="overflow"/>
						</markMenu>
					</markMenuItem>
					<markMenuItem text="Customize Material Editor" position="overflow">
